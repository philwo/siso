rule action
  command = python3 ../../tools/action.py ${in} ${out}
  restat = 1

rule stamp
  command = python3 ../../tools/stamp.py ${out}

build gen/foo.srcjar: action ../../foo.txt

build foo.stamp: stamp gen/foo.srcjar

build all: phony foo.stamp

build build.ninja: phony
