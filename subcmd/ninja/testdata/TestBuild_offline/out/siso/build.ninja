rule gen1
  command = python3 ../../tools/gen.py --output=${out}
build output1: gen1

rule gen2
  command = python3 ../../tools/gen.py --input=${in} --output=${out}
build output2: gen2 output1

build all: phony output2

build build.ninja: phony

default all
