rule action
  command = python3 ../../tools/action.py ${in} ${out}

build obj/foo0.inputdeps.out: action ../../foo/0.input
build obj/foo1.inputdeps.out: action ../../foo/1.input
build obj/foo.inputdeps.out: phony obj/foo0.inputdeps.out obj/foo1.inputdeps.out
build gen/asserts.out: action ../../foo/assert.in

build all: phony gen/asserts.out obj/foo.inputdeps.out

build build.ninja: phony

default all
