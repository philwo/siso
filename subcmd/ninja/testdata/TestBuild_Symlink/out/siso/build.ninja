rule action
   command = python3 ../../tools/action.py ${out}

rule symlink
   command = python3 ../../tools/symlink.py ${in} ${out}
   restat = 1

build out1: action | ../../tools/action.py
build out2: symlink out1 | ../../tools/symlink.py
build all: phony out1 out2

build build.ninja: phony

