rule stamp
  command = python3 ../../tools/touch.py ${out}

rule noop
  command = python3 ../../tools/noop.py

build sdk/xcode_links/info.plist: noop
build obj/copy.stamp: stamp sdk/xcode_links/info.plist

build all: phony obj/copy.stamp

build build.ninja: phony
