rule objcxx
  command = python3 ../../tools/clang++.py -MMD -MF ${out}.d -F ../../build/mac_files/SDKs/MacOSX14.0.sdk/System/Library/PrivateFrameworks -isysroot ../../build/mac_files/SDKs/MacOSX14.0.sdk -c ${in} -o ${out}
  deps = gcc
  depfile = ${out}.d

build obj/mac/foo.o: objcxx ../../mac/foo.mm

build all: phony obj/mac/foo.o

build build.ninja: phony
